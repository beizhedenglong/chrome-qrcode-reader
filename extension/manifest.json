{
  "name": "QR Code Reader",
  "description": "A chrome extension for reading QR code from webpage.",
  "version": "1.0.0",
  "browser_action": {
    "default_popup": "dist/popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S"
      },
      "description": ""
    }
  }
}